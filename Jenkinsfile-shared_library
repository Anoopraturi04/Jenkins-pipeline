@Library('sharedlibrary')
pipeline{
    agent any
    tools{
       maven 'mymaven'
    }
    stages{
        stage('Clone Repo'){
           repo('https://github.com/Anoopraturi04/shared_library_jenkins.git')
           //here "repo" is the grovvy inside the Shared Library repository that we are using here in this pipline
        }
    }
}